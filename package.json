{
  "name": "wat2wasm",
  "version": "2.0.0",
  "description": "WASM/JS clone of wabt's wat2wasm",
  "main": "index.js",
  "bin": {
    "wat2wasm": "index.js"
  },
  "dependencies": {
    "concat-stream": "^1.6.2",
    "minimist": "^1.2.0"
  },
  "devDependencies": {
    "standard": "^12.0.1",
    "tape": "^4.11.0",
    "wasm2js": "^0.2.0"
